    "i": "^0.3.7",
    "jalali-moment": "^3.3.11",
    "jotai": "^2.9.1",
    "js-cookie": "^3.0.5",
    "json-server": "1.0.0-beta.1",
    "nanoid": "^5.0.7",
    "npm": "^10.9.0",
    "react": "^18.3.1",
    "react-date-object": "^2.1.9",
